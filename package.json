{
  "name": "cdkapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --verbose",
    "tw": "jest --watch --verbose",
    "integ": "jest -c jest.config-integ.js --verbose --runInBand",
    "integw": "jest -c jest.config-integ.js --verbose --watch --runInBand",
    "tsc": "tsc",
    "eslint": "eslint",
    "lint": "eslint --ignore-path .eslintignore . --ext ts --ext tsx --quiet --fix",
    "postinstall": "bash ./zscripts/npm_postinstall.bash",
    "layers": "cd layers/nodejs && npm install",
    "build": "npm run layers && rm -rf dist && rollup --config rollup.config-lambda.js",
    "cdk": "npm run build && bash ./zscripts/cdk.bash"
  },
  "devDependencies": {
    "@cpmech/az-cdk": "^8.4.0",
    "@cpmech/az-cognito": "^3.31.0",
    "@cpmech/az-dynamo": "^3.34.0",
    "@cpmech/az-lambda": "^3.29.0",
    "@cpmech/az-senqs": "^3.30.0",
    "@cpmech/envars": "^2.24.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "aws-cdk": "^1.89.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2"
  }
}